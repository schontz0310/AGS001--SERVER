{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/shared/infra/http/server.js",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "knex:Mrun": "knex migrate:latest --knexfile src/shared/infra/database/knex/knexfile.ts migrate:latest",
    "knex:Mcreate": "knex migrate:make --knexfile src/shared/infra/database/knex/knexfile.ts",
    "knex:Smake": "knex seed:make --knexfile src/shared/infra/database/knex/knexfile.ts",
    "knex:Srun": "knex seed:run --knexfile src/shared/infra/database/knex/knexfile.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cors": "^2.8.9",
    "@types/ioredis": "^4.28.8",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mime": "^2.0.3",
    "@types/node": "^17.0.13",
    "@types/nodemailer": "^6.4.2",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "class-transformer": "^0.5.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/cli": "^7.15.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/multer": "^1.4.5",
    "async-mqtt": "^2.6.2",
    "aws-sdk": "^2.829.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^14.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "ioredis": "^4.27.9",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.16",
    "mime": "^2.5.2",
    "mqtt": "^4.3.4",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.4.0",
